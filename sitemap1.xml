---
layout: sitemap
title : sitemap
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>http://itxuye.com</loc>
       
    </url>
    {% for post in site.posts %}
    <url>
        <loc>http://itxuye.com{{ post.url | remove: 'index.html' }}</loc>
       
    </url>
    {% endfor %}

    {% for page in site.pages %}
    {% if page.layout != nil %}
    {% if page.layout != 'feed' %}
    <url>
        <loc>http://itxuye.com{{ page.url | remove: 'index.html' }}</loc>
       
    </url>
    {% endif %}
    {% endif %}
    {% endfor %}
</urlset>
